-------------------------------------------------------------------------
PART A - SQL statements creating the tables for the database
-------------------------------------------------------------------------

#creates a database called library if it does not already exist
CREATE DATABASE IF NOT EXISTS library;


/* creates table if it doesnt already exist called Author, with the field names AuthorID, AuthorFirstName, AuthorLastName, AuthorNationality */
/* authorID is the primary key of the table */
CREATE TABLE IF NOT EXISTS Author (
	AuthorID INT(10),
  	AuthorFirstName VARCHAR(50),
 	AuthorLastName VARCHAR(50),
  	AuthorNationality VARCHAR(50),
  	
	PRIMARY KEY (authorID)
);


/* creates table if it doesnt already exist called Book, with the field names BookID, BookTitle, AuthorID, Genre */
/* bookID is the primary key of this table */
/* authorID is the foreign key of this table, relating it to the author table by the common field authorID */
CREATE TABLE IF NOT EXISTS Book (
	BookID INT(10),
	BookTitle VARCHAR(100),
	AuthorID INT(10),
	Genre VARCHAR(50),

	PRIMARY KEY (BookID),
	FOREIGN KEY (AuthorID) REFERENCES Author(AuthorID) 
);


/* creates table if it doesnt already exist called client, with the field names ClientID, ClientFirstName,CLientLastName, ClientDOB, Occupation */
/* clientID is the primary key of the table */
CREATE TABLE IF NOT EXISTS Client(
	ClientID INT(10),
	ClientFirstName VARCHAR(50),
	ClientLastName VARCHAR(50),
	ClientDOB YEAR,
	Occupation varchar(100),
	
	PRIMARY KEY (ClientID)
);


/* creates table if it doesnt already exist called borrower, with the field names borrowID, clientID, bookID, BorrowDate */
/* borrowID is the primary key of this table */
/* client is the foreign key of this table, relating it to the client table by the common field clientID */
CREATE TABLE IF NOT EXISTS Borrower(
	BorrowID INT(10),
	ClientID INT(10),
	BookID INT(10),
    	BorrowDate DATE,
    
	PRIMARY KEY(BorrowID),
    	FOREIGN KEY (BookID) REFERENCES Book(BookID),
    	FOREIGN KEY (ClientID) REFERENCES Client(ClientID) 
);


/* data types above follow the fields to specify the required data type for that field. Breakdown as follows:
	INT(10) - integer with limit size of 10 digits
	VARCHAR(50) - string character that has various sizes, with a limit size of 50 characters
	YEAR - year 
	DATE - date with integers shown as YYYY-MM-DD

*/
	



#Creating Indexes

/* creates a unique index named authorIndex on author first and last name fields in the author table. Index created for faster queries */
CREATE UNIQUE INDEX authorIndex
ON author (authorFirstName, authorLastName);

/* creates a unique index named bookIndex on the bookTitle and genre fields in the book table. Index created for faster queries */
CREATE UNIQUE INDEX bookIndex
ON book (bookTitle, genre);

/* creates a unique index named clientIndex on the client first & last names, client DOB, & occupation fields in the client table. Index created for faster queries */
CREATE UNIQUE INDEX clientIndex
ON client (clientFirstName, clientLastName, clientDOB, occupation);

/* creates a unique index named borrowerIndex on the clientID and borrowDate fields in the borrower table */
CREATE UNIQUE INDEX borrowerIndex
ON borrower (clientID, borrowDate);



